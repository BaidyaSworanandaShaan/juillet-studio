{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "mobileNumber": {
      "type": "biginteger"
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "insideValley",
        "outsideValley"
      ]
    },
    "detailedAddress": {
      "type": "text"
    },
    "products": {
      "type": "json"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Shipped",
        "Confirmed"
      ]
    }
  }
}
